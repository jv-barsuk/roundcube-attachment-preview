{
    "name": "meles/attachment_preview",
    "type": "roundcube-plugin",
    "description": "Preview Attachment in message area instead of new tab",
    "keywords": ["mail","roundcube","attachments"],
    "homepage": "https://github.com/jv-barsuk/roundcube_attachment_preview",
    "version": "0.1.0",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "meles",
            "email": "nowhere@example.com",
            "homepage": "https://github.com/jv-barsuk",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "roundcube/plugin-installer": ">=0.1.3"
    },
    "scripts": {
        "lint": "php-cs-fixer -v fix --diff --diff-format udiff --dry-run",
        "fix": "php-cs-fixer fix"
    },
    "extra": {
        "roundcube": {
            "min-version": "1.1"
        }
    }
}